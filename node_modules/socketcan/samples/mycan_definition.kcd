<NetworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="Definition.xsd"
 version="0.3">
	<Document name="CANBUS for smartfarm" version="1.00" author="Andy Ahn" company="KW Univ." date="2019-08-08">
	A CANBUS connection for a simple smartfarm conditions.
	</Document> 
	<Node id="1" name="House1"/>
	<Node id="2" name="House2"/>
	<Node id="3" name="CommUnit"/>
	
	<Bus name="FarmBUS" baudrate="125000">
		
		<Message id="0x5A2" name="TankController" interval="200">
			<Producer>
				<NodeRef id="1"/>
			</Producer>
			<Signal name="TankLevel" offset="0" length="16"/>
			<Signal name="TankTemperature" offset="16" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
			<Signal name="FillingStatus" offset="32" length="4"/>
		</Message>
	</Bus>
</NetworkDefinition>


