<NetworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="Definition.xsd"
 version="0.3">
	<Document name="CANBUS for smartfarm" version="1.00" author="Andy Ahn" company="KW Univ." date="2019-08-08">
	A CANBUS connection for a simple smartfarm conditions.
	</Document> 
	<Node id="1" name="House1"/>
	<Node id="2" name="House2"/>
	<Node id="3" name="CommUnit"/>
	
	<Bus name="FarmBUS" baudrate="125000">
		
		<Message id="0x001" name="House1Stat" interval="200">
			<Producer>
				<NodeRef id="1"/>
			</Producer>
			<Signal name="temperature1" offset="0" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
			<Signal name="temperature2" offset="16" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
			<Signal name="humidity1" offset="32" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
			<Signal name="humidity2" offset="48" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
		</Message>

		<Message id="0x002" name="House2Stat" interval="200">
			<Producer>
				<NodeRef id="2"/>
			</Producer>
			<Signal name="temperature1" offset="0" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
			<Signal name="temperature2" offset="16" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
			<Signal name="humidity1" offset="32" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
			<Signal name="humidity2" offset="48" length="16">
				<Value unit="Cel" type="signed"/>
			</Signal>
		</Message>
	</Bus>
</NetworkDefinition>


